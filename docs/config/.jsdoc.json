{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "include": ["src/js", "package.json"],
        "includePattern": ".js$",
        "excludePattern": "(node_modules/|docs)"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": true
    },
    "opts": {
        "destination": "./docs",
        "encoding": "utf8",
        "recurse": true,
        "private": false,
        "readme": "./docs/docs.md",
        "template": "./node_modules/docdash"
    }
}
